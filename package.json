{
  "name": "@szhsin/react-accordion",
  "version": "1.2.3",
  "description": "The complete accordion solution for React.",
  "author": "Zheng Song",
  "license": "MIT",
  "repository": "szhsin/react-accordion",
  "homepage": "https://szhsin.github.io/react-accordion/",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist/",
    "types/"
  ],
  "keywords": [
    "react",
    "accordion",
    "component",
    "hook",
    "unstyled",
    "headless UI",
    "design system",
    "accessibility",
    "wai-aria"
  ],
  "scripts": {
    "start": "run-p watch \"types -- --watch\"",
    "eg": "npm run dev --prefix example",
    "bundle": "rollup -c",
    "watch": "rollup -c -w",
    "clean": "rm -Rf dist types",
    "post-build": "rm -Rf types/__tests__",
    "types": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pret": "prettier -c .",
    "pret:fix": "prettier -w .",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:cov": "jest --coverage=true",
    "build": "run-s pret clean lint types bundle post-build"
  },
  "peerDependencies": {
    "react": "^16.14 || ^17.0 || ^18.0",
    "react-dom": "^16.14 || ^17.0 || ^18.0"
  },
  "dependencies": {
    "react-transition-state": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.34",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-plugin-pure-annotations": "^0.1.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-hooks-addons": "^0.3.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.3.0",
    "typescript": "^5.2.2"
  }
}
